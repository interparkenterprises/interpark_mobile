{
  "expo": {
    "owner": "masterkelvin",
    "name": "InterPark",
    "slug": "InterPark",
    "version": "1.0.0",
    "orientation": "portrait",
    "jsEngine": "jsc",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*",
      "assets/fonts/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.interpark.realestate",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to the camera to upload property images.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to upload property images.",
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "interpark-auth",
            "CFBundleURLSchemes": [
              "interpark"
            ]
          }
        ]
      }
    },
    "android": {
      "package": "com.interpark.realestate",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "INTERNET",
        "NOTIFICATIONS",
        "VIBRATE",
        "RECEIVE_BOOT_COMPLETED",
        "WAKE_LOCK"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "interpark"
            }
          ],
          "category": [
            "DEFAULT",
            "BROWSABLE"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "apiUrl": "${EXPO_PUBLIC_API_BASE_URL}",
      "eas": {
        "projectId": "db6634eb-4587-4a79-bbd7-cc68a84471f5"
      }
    },
    "scheme": "interpark",
    "newArchEnabled": true,
    "notification": {
      "icon": "./assets/adaptive-icon.png",
      "color": "#005478",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "InterPark"
    },
    "plugins": [
      "expo-asset",
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": false
          }
        }
      ],
      "expo-router",
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/adaptive-icon.png",
          "color": "#005478",
          "defaultChannel": "default"
        }
      ]
    ]
  }
}
