{
  "expo": {
    "name": "InterPark",
    "slug": "InterPark",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*",
      "assets/fonts/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.interpark.realestate",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to the camera to upload property images.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to upload property images.",
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": ["interpark"]
          }
        ]
      }
    },
    "android": {
      "package": "com.interpark.realestate",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "INTERNET"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "interpark"
            }
          ],
          "category": ["DEFAULT", "BROWSABLE"]
        }
      ],
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "apiUrl": "http://10.0.2.2:8000/api",
      "eas": {
        "projectId": "db6634eb-4587-4a79-bbd7-cc68a84471f5"
      },
      "googleClientIdIos": "865590816188-fh0c7p79r1jj2es6cuqkqkoblltu7i3f.apps.googleusercontent.com",
      "googleClientIdAndroid": "865590816188-ieupcun4rbinoe0csmjmvraf4sgen4mp.apps.googleusercontent.com",
      "googleClientIdWeb": "865590816188-fh0c7p79r1jj2es6cuqkqkoblltu7i3f.apps.googleusercontent.com"
    },
    "scheme": "interpark",
    "newArchEnabled": false,
    "notification": {
      "icon": "./assets/adaptive-icon.png"
    },
    "plugins": [
      "expo-asset",
      [
        "expo-build-properties",
        {
          "android": {
            "googleServicesFile": "./google-services.json"
          }
        }
      ]
    ]
  }
}
